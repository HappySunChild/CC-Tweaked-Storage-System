{
	"bundle": {
		"excludes": [
			"cc/**"
		],
		"require_mode": {
			"name": "path",
			"sources": {
				"lib": "src/lib",
				"prompt": "src/prompt",
				"menus": "src/menus",
				"utility": "src/utility"
			}
		}
	},
	"generator": "dense",
	"rules": [
		"remove_types",
		"compute_expression",
		"remove_nil_declaration",
		"remove_unused_if_branch",
		"remove_unused_variable",
		"filter_after_early_return",
		{
			"rule": "rename_variables",
			"include_functions": true
		}
	]
}